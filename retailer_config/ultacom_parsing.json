{
  "fetch_products": {
    "selectors": [
      { "type": "classname", "value": "productQvContainer" },
      { "type": "classname", "value": "ProductCard" }
    ]
  },
  "fetch_product": [
    {
      "field": "product_id",
      "field_name": "Product ID",
      "selectors": [{ "type": "attribute", "value": "id" }]
    },
    {
      "field": "country",
      "field_name": "Country",
      "selectors": [{ "type": "config_value", "value": "country" }]
    },
    {
      "field": "retailer",
      "field_name": "Retailer",
      "selectors": [{ "type": "config_value", "value": "retailer" }]
    },
    {
      "field": "department",
      "field_name": "Department",
      "selectors": [{ "type": "config_value", "value": "department" }]
    },
    {
      "field": "category",
      "field_name": "Category",
      "selectors": [{ "type": "config_value", "value": "category" }]
    },
    {
      "field": "page",
      "field_name": "Page",
      "selectors": [{ "type": "config_value", "value": "page" }]
    },
    {
      "field": "device",
      "field_name": "Device",
      "selectors": [{ "type": "config_value", "value": "device" }]
    },
    {
      "field": "page_url",
      "field_name": "Page URL",
      "selectors": [{ "type": "config_value", "value": "page_url" }]
    },
    {
      "field": "brand",
      "field_name": "Brand",
      "selectors": [
        { "type": "classname", "value": "prod-title" },
        { "type": "classname", "value": "ProductCard__brand" }
      ]
    },
    {
      "field": "product_name",
      "field_name": "Product Name",
      "selectors": [
        { "type": "classname", "value": "prod-desc" },
        { "type": "classname", "value": "ProductCard__product" }
      ]
    },
    {
      "field": "sku",
      "field_name": "SKU",
      "selectors": []
    },
    {
      "field": "position",
      "field_name": "Position",
      "selectors": []
    },
    {
      "field": "product_page_url",
      "field_name": "Product Page URL",
      "selectors": [
        {
          "type": "classname_xpath_attribute",
          "classname": "prod-desc",
          "xpath": "./a",
          "selector_attribute": "href"
        },
        {
          "type": "tagname_attribute",
          "value": "a",
          "selector_attribute": "href"
        }
      ]
    },
    {
      "field": "listing_label",
      "field_name": "Label",
      "selectors": [
        { "type": "classname", "value": "circleBase1" },
        { "type": "classname", "value": "ProductCard__badge" },
        {
          "type": "classname_xpath_attribute",
          "classname": "circleBase1",
          "xpath": "./img",
          "selector_attribute": "alt"
        }
      ]
    },
    {
      "field": "reviews",
      "field_name": "Reviews",
      "selectors": [
        { "type": "delete_classname", "value": "sr-only" },
        {
          "type": "classname_xpath",
          "classname": "ReviewStars",
          "xpath": "./span[1]"
        }
      ]
    },
    {
      "field": "ratings",
      "field_name": "Ratings",
      "selectors": [
        {
          "type": "classname_xpath",
          "classname": "ReviewStars",
          "xpath": "./span[2]"
        }
      ]
    },
    {
      "field": "date",
      "field_name": "Date",
      "selectors": [{ "type": "config_value", "value": "date" }]
    }
  ]
}
