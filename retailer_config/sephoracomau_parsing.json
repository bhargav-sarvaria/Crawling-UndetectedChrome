    {
      "field": "reviews",
      "field_name": "Reviews",
      "selectors": [{ "type": "classname", "value": "product-rating-count" }]
    },
    {
      "field": "ratings",
      "field_name": "Ratings",
      "selectors": [
        {
          "type": "classname_attribute",
          "value": "rateit-range",
          "selector_attribute": "aria-valuenow"
        }
      ]
    },
    {
      "field": "date",
      "field_name": "Date",
      "selectors": [{ "type": "config_value", "value": "date" }]
    }
  ]
