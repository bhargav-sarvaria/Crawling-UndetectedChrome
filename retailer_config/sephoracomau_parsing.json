{
  "fetch_products": {
    "selectors": [{ "type": "classname", "value": "product-card" }]
  },
  "fetch_product": [
    {
      "field": "product_id",
      "field_name": "Product ID",
      "selectors": [{ "type": "attribute", "value": "data-variant-id" }]
    },
    {
      "field": "country",
      "field_name": "Country",
      "selectors": [{ "type": "config_value", "value": "country" }]
    },
    {
      "field": "retailer",
      "field_name": "Retailer",
      "selectors": [{ "type": "config_value", "value": "retailer" }]
    },
    {
      "field": "department",
      "field_name": "Department",
      "selectors": [{ "type": "config_value", "value": "department" }]
    },
    {
      "field": "category",
      "field_name": "Category",
      "selectors": [{ "type": "config_value", "value": "category" }]
    },
    {
      "field": "page",
      "field_name": "Page",
      "selectors": [{ "type": "config_value", "value": "page" }]
    },
    {
      "field": "device",
      "field_name": "Device",
      "selectors": [{ "type": "config_value", "value": "device" }]
    },
    {
      "field": "page_url",
      "field_name": "Page URL",
      "selectors": [{ "type": "config_value", "value": "page_url" }]
    },
    {
