    {
      "field": "position",
      "field_name": "Position",
      "selectors": []
    },
    {
      "field": "product_page_url",
      "field_name": "Product Page URL",
      "selectors": [
        {
          "type": "classname_attribute",
          "value": "product-card-image-container",
          "selector_attribute": "href"
        }
      ]
    },
    {
      "field": "listing_label",
      "field_name": "Label",
      "selectors": []
