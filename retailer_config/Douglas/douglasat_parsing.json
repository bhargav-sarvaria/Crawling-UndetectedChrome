{
  "fetch_products": {
    "selectors": [{ "type": "classname", "value": "product-tile" }]
  },
  "wait_for_class": "product-tile",
  "fetch_product": [
    {
      "field": "product_id",
      "field_name": "Product ID",
      "selectors": [
        {
          "type": "classname_attribute_split",
          "classname": "product-tile__main-link",
          "selector_attribute": "href",
          "splitter": "/",
          "split": -1
        }
      ]
    },
    {
      "field": "country",
      "field_name": "Country",
      "selectors": [{ "type": "config_value", "value": "country" }]
    },
    {
      "field": "retailer",
      "field_name": "Retailer",
      "selectors": [{ "type": "config_value", "value": "retailer" }]
    },
    {
      "field": "department",
      "field_name": "Department",
      "selectors": [{ "type": "config_value", "value": "department" }]
    },
    {
      "field": "category",
      "field_name": "Category",
      "selectors": [{ "type": "config_value", "value": "category" }]
    },
    {
      "field": "page",
      "field_name": "Page",
      "selectors": [{ "type": "config_value", "value": "page" }]
    },
    {
      "field": "device",
      "field_name": "Device",
      "selectors": [{ "type": "config_value", "value": "device" }]
    },
    {
      "field": "page_url",
      "field_name": "Page URL",
      "selectors": [{ "type": "config_value", "value": "page_url" }]
    },
    {
      "field": "brand",
      "field_name": "Brand",
      "selectors": [{ "type": "classname", "value": "product-tile__top-brand" }]
    },
    {
      "field": "product_name",
      "field_name": "Product Name",
      "selectors": [
        {
          "type": "classnames",
          "values": ["product-tile__brand-line", "product-tile__name"]
        }
      ]
    },
    {
      "field": "sku",
      "field_name": "SKU",
      "selectors": [
        {
          "type": "classname",
          "value": "product-price__extended-content-units"
        }
      ]
    },
    {
      "field": "position",
      "field_name": "Position",
      "selectors": []
    },
    {
      "field": "product_page_url",
      "field_name": "Product Page URL",
      "selectors": [
        {
          "type": "classname_attribute",
          "value": "product-tile__main-link",
          "selector_attribute": "href"
        }
      ]
    },
    {
      "field": "listing_label",
      "field_name": "Label",
      "selectors": [{ "type": "classname", "value": "eyecatcher" }]
    },
    {
      "field": "reviews",
      "field_name": "Reviews",
      "selectors": [
        {
          "type": "classname_split",
          "classname": "product-tile__ratings-info",
          "splitter": " (",
          "split": 1
        }
      ]
    },
    {
      "field": "ratings",
      "field_name": "Ratings",
      "selectors": [
        {
          "type": "classname_split",
          "classname": "product-tile__ratings-info",
          "splitter": " (",
          "split": 0
        }
      ]
    },
    {
      "field": "date",
      "field_name": "Date",
      "selectors": [{ "type": "config_value", "value": "date" }]
    },
    {
      "field": "sponsored_flag",
      "field_name": "Sponsored Flag",
      "selectors": []
    }
  ]
}
